{
  "name": "bytic/http-error-pages",
  "type": "library",
  "description": "Php Error page generator for Apache",
  "homepage": "https://github.com/bytic/http-error-pages",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\HttpErrorPages\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\HttpErrorPages\\Tests\\": "tests/src",
      "ByTIC\\HttpErrorPages\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "composer/composer": "^1.0",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "bytic/controllers": "^0.9|^1.0",
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  },
  "scripts": {
    "build": "ByTIC\\HttpErrorPages\\Composer\\ScriptHandler::build"
  }
}
